{
    "manifest_version": 3,
    "name": "Auto Unique Random Search",
    "version": "1.0",
    "description": "Automatically performs unique random searches with delay.",
    "permissions": ["scripting", "tabs"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
      },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png"
      }
    }
  }
  